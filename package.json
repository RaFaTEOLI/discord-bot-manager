{
  "name": "discord-manager-bot",
  "version": "1.0.2",
  "description": "Discord Bot and Rest API",
  "author": "Rafael Tessarolo",
  "license": "MIT",
  "devDependencies": {
    "auto-changelog": "^2.3.0"
  },
  "scripts": {
    "changelog": "auto-changelog --template changelog-template.hbs -p -u --commit-limit false",
    "changelog-debug": "auto-changelog --template changelog-template.hbs -p --template json --output changelog-data.json"
  },
  "auto-changelog": {
    "commitLimit": false,
    "unreleased": true,
    "replaceText": {
      "[Ff]eature:": "",
      "[Ff]ix:": "",
      "[Bb]reak:": ""
    },
    "includeBranch": [
      "main"
    ]
  }
}
