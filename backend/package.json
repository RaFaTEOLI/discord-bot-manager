{
  "name": "backend",
  "version": "1.0.0",
  "description": "Discord Bot and Rest API",
  "main": "index.js",
  "author": "Rafael Tessarolo",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev": "tsnd -r tsconfig-paths/register --inspect --transpile-only --ignore node_modules src/index.ts",
    "server": "json-server --watch src/db.json",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "format": "yarn prettier --write .",
    "format:check": "npx prettier --check .",
    "lint": "yarn eslint src/*"
  },
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "axios": "^0.21.1",
    "cors": "^2.8.5",
    "date-fns": "^2.21.1",
    "discord-music-player": "^7.2.0",
    "discord.js": "^12.5.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "ffmpeg": "^0.0.4",
    "firebase": "^9.5.0",
    "fluent-ffmpeg": "^2.1.2",
    "json-server": "^0.16.3",
    "opusscript": "^0.0.8",
    "ytdl-core": "^4.8.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.12",
    "@types/ffmpeg": "^1.0.4",
    "@types/firebase": "^3.2.1",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/json-server": "^0.14.3",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "axios": "^0.21.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "prettier": "^2.5.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.4"
  }
}
